{
  "name": "plugin-monitor-frontend",
  "version": "1.0.0",
  "description": "",
  "author": "duuliy (715181149@qq.com)",
  "scripts": {
    "electron-start": "electron .",
    "electron-start:dev": "electron . dev",
    "electron-build": "electron-builder",
    "packager-win": "electron-packager ./dist duuliyTest --platform=win32  --arch=x64 --electron-version=1.0.0 --out=./build --overwrite",
    "start": "cross-env NODE_ENV=dev webpack serve --progress --config webpack.config.js",
    "build": "cross-env CLIENT_ENV=production NODE_ENV=production webpack --config webpack.config.js",
    "test": "rimraf coverage && cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  },
  "main": "main.js",
  "homepage": ".",
  "license": "MIT",
  "build": {
    "appId": "duuliy.app",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "icon.ico"
    },
    "files": [
      "dist/**/*",
      "main.js"
    ],
    "extends": null
  },
  "jest": {
    "collectCoverage": false,
    "coverageThreshold": {
      "global": { 
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    },
    "collectCoverageFrom": [
      "__tests__/**/*.{js,jsx}",
      "!__tests__/**/*.test.{js,jsx}"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transformIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "moduleDirectories": [
      "node_modules",
      "__tests__",
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/enzyme.config.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest",
      "^.+\\.css$": "<rootDir>/testBuild/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/testBuild/fileTransform.js"
    },
    "testPathIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$",
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/testBuild/fileMock.js",
      "\\.(css|less)$": "<rootDir>/testBuild/styleMock.js"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.12.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "husky": "^4.2.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "antd": "^4.10.3",
    "autoprefixer": "^8.2.0",
    "axios": "^0.21.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "electron": "^13.1.2",
    "electron-builder": "^21.2.0",
    "electron-packager": "^15.2.0",
    "enzyme": "^3.11.0",
    "file-loader": "^6.2.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^4.5.1",
    "jest": "^26.6.3",
    "less": "^4.1.0",
    "less-loader": "^7.2.1",
    "lodash": "^4.17.20",
    "lodash-webpack-plugin": "^0.11.6",
    "mini-css-extract-plugin": "^1.3.4",
    "mocker-api": "^2.7.5",
    "moment": "^2.29.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.1.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "svg-sprite-loader": "^5.2.1",
    "svgo-loader": "^2.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "4.44.2",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
